                                  &lt;p&gt;&lt;a href=&quot;/parts/<%= s.part_id %>&quot;&gt;<%= s.part.manufacturer_model_number %>&lt;/a&gt;&lt;/p&gt;
                                <% else %>
                                  <p></p>
                                <% end %>
                              <% else %>
                                <p>No Sku's Present</p>
                              <% end %>
                            <% end %>"
          data-child-description="<% i.stockmovements.each do |s| %>
                                    <% if s.is_product %>
