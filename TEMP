                                  &lt;p&gt;&lt;a href=&quot;/products/<%= s.product_id %>&quot;&gt;<%= s.product.manufacturer_model_number %>&lt;/a&gt;&lt;/p&gt;
                                <% else %>
                                  <p></p>
                                <% end %>
                              <% elsif s.is_part %>
