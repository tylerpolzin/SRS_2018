<p id="notice"><%= notice %></p>

<h1>Upload Batches</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_batches.each do |upload_batch| %>
      <tr>
        <td><%= link_to 'Show', upload_batch %></td>
        <td><%= link_to 'Edit', edit_upload_batch_path(upload_batch) %></td>
        <td><%= link_to 'Destroy', upload_batch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload Batch', new_upload_batch_path %>
