<p id="notice"><%= notice %></p>

<h1>Stockmovement Batches</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stockmovement_batches.each do |stockmovement_batch| %>
      <tr>
        <td><%= link_to 'Show', stockmovement_batch %></td>
        <td><%= link_to 'Edit', edit_stockmovement_batch_path(stockmovement_batch) %></td>
        <td><%= link_to 'Destroy', stockmovement_batch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stockmovement Batch', new_stockmovement_batch_path %>
